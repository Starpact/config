{
  "editor.semanticHighlighting.enabled": true,
  "editor.fontSize": 16,
  "editor.cursorSurroundingLines": 10,
  "editor.formatOnSave": true,
  //
  "terminal.integrated.fontSize": 16,
  //
  "workbench.colorTheme": "Gruvbox Dark Medium",
  //
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  //
  "rust-analyzer.inlayHints.maxLength": 10,
  "rust-analyzer.completion.addCallArgumentSnippets": false,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.inlayHints.chainingHints": false,
  "rust-analyzer.inlayHints.parameterHints": false,
  //
  "go.testFlags": [
    "-v",
    "-race",
    "--count=1"
  ],
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ],
  //
  "gitlens.advanced.blame.delayAfterEdit": 0,
  //
  "vim.leader": "<space>",
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    }
  ],
  "vim.handleKeys": {
    "<C-f>": true,
    "<C-w>": false
  },
  "vim.easymotion": true,
  //
  "cSpell.userWords": [
    "Actix",
    "CMDB",
    "Deque",
    "Echarts",
    "Hongjie",
    "Mutex",
    "Spex",
    "Starpact",
    "Xlsx",
    "awsl",
    "azip",
    "bakana",
    "cmath",
    "colormap",
    "consts",
    "curr",
    "erfc",
    "eval",
    "ffmpeg",
    "garena",
    "ictx",
    "interp",
    "libc",
    "libm",
    "nalgebra",
    "ncols",
    "ndarray",
    "nolint",
    "nrows",
    "onclick",
    "oneshot",
    "postprocess",
    "powf",
    "preload",
    "preprocess",
    "promisified",
    "shopee",
    "simd",
    "sjtu",
    "struct",
    "tauri",
    "thiserror",
    "vals",
    "vmax",
    "vmin",
    "winres"
  ],
  "background.filepath": "d:\\yhj\\Pictures\\WallPaper\\6.png",
  "background.opacity": 0.9,
  "cSpell.enableFiletypes": [
    "toml"
  ]
}
