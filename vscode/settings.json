{
  "editor.semanticHighlighting.enabled": true,
  "editor.fontSize": 15,
  "editor.cursorSurroundingLines": 10,
  "editor.formatOnSave": true,
  "editor.fontFamily": "Monaco, 'Courier New', monospace",
  "editor.minimap.enabled": false,
  //
  "terminal.integrated.fontSize": 15,
  //
  "workbench.iconTheme": "material-icon-theme",
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Gruvbox Dark Medium",
  "oneDarkPro.italic": false,
  //
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  //
  "rust-analyzer.inlayHints.maxLength": 10,
  "rust-analyzer.completion.addCallArgumentSnippets": false,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.inlayHints.chainingHints": false,
  "rust-analyzer.inlayHints.parameterHints": false,
  //
  "go.testFlags": [
    "-v",
    "-race",
    "--count=1"
  ],
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ],
  //
  "vim.smartRelativeLine": true,
  "vim.leader": "<space>",
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.handleKeys": {
    "<C-f>": true,
    "<C-w>": false
  },
  //
  "editor.tokenColorCustomizations": {
    "[Gruvbox Dark Medium]": {
      "textMateRules": [
        {
          "scope": [
            "variable.other.assignment.go",
            "variable.other.declaration.go",
            "entity.name.package.go",
            "variable.other",
            "variable.parameter"
          ],
          "settings": {
            "foreground": "#ebdbb2"
          }
        },
        {
          "scope": [
            "keyword.struct.go",
            "keyword.package.go",
            "keyword.interface.go",
            "keyword.import.go",
            "storage.modifier"
          ],
          "settings": {
            "foreground": "#fb4934"
          }
        },
        {
          "scope": [
            "entity.name.type.go"
          ],
          "settings": {
            "foreground": "#fabd2f"
          }
        },
        {
          "scope": [
            "entity.name.function.go",
            "support.function.go",
            "support.function.builtin.go",
            "entity.name.function"
          ],
          "settings": {
            "foreground": "#b8bb26"
          }
        },
        {
          "scope": [
            "variable.other.enummember"
          ],
          "settings": {
            "foreground": "#83a598"
          }
        },
        {
          "scope": [
            "comment"
          ],
          "settings": {
            "fontStyle": ""
          }
        }
      ]
    },
    "[One Dark Pro]": {
      "textMateRules": [
        {
          "scope": [
            "support.function.go",
            "support.function.builtin.go"
          ],
          "settings": {
            "foreground": "#61afef"
          }
        },
        {
          "scope": [
            "keyword.operator.assignment.go",
            "variable.other.declaration.go",
            "variable.other.assignment.go"
          ],
          "settings": {
            "foreground": "#abb2bf"
          }
        },
        {
          "scope": [
            "storage.type"
          ],
          "settings": {
            "foreground": "#e5c07b"
          }
        }
      ]
    }
  },
  //
  "background.useDefault": false,
  "background.customImages": [
    "https://user-images.githubusercontent.com/24371189/47023104-85336080-d191-11e8-957f-b4ac3f8a24c1.png",
    "https://user-images.githubusercontent.com/24371189/47023104-85336080-d191-11e8-957f-b4ac3f8a24c1.png"
  ],
  "background.style": {
    "content": "''",
    "pointer-events": "none",
    "position": "absolute",
    "z-index": "99999",
    "right": "0",
    "bottom": "0",
    "width": "100%",
    "height": "100%",
    "background-size": "30%",
    "opacity": 0.35
  },
  //
