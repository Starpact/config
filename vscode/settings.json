{
  "editor.semanticHighlighting.enabled": true,
  "editor.fontSize": 15,
  "editor.cursorSurroundingLines": 10,
  "editor.formatOnSave": true,
  "editor.fontFamily": "Monaco, monospace",
  //
  "terminal.integrated.fontSize": 15,
  //
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "Gruvbox Dark Medium",
  //
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  //
  "rust-analyzer.inlayHints.maxLength": 10,
  "rust-analyzer.completion.addCallArgumentSnippets": false,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.inlayHints.chainingHints": false,
  "rust-analyzer.inlayHints.parameterHints": false,
  //
  "go.testFlags": [
    "-v",
    "-race",
    "--count=1"
  ],
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ],
  //
  "vim.leader": "<space>",
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    }
  ],
  "vim.handleKeys": {
    "<C-f>": true,
    "<C-w>": false
  },
  //
  "editor.tokenColorCustomizations": {
    "[Gruvbox Dark Medium]": {
      "textMateRules": [
        {
          "scope": [
            "variable.other.assignment.go",
            "variable.other.declaration.go",
            "entity.name.package.go",
            "variable.other",
            "variable.parameter"
          ],
          "settings": {
            "foreground": "#ebdbb2"
          }
        },
        {
          "scope": [
            "keyword.struct.go",
            "keyword.package.go",
            "keyword.interface.go",
            "keyword.import.go"
          ],
          "settings": {
            "foreground": "#fb4934"
          }
        },
        {
          "scope": [
            "entity.name.type.go"
          ],
          "settings": {
            "foreground": "#fabd2f"
          }
        },
        {
          "scope": [
            "entity.name.function.go",
            "support.function.go",
            "support.function.builtin.go",
            "entity.name.function",
          ],
          "settings": {
            "foreground": "#b8bb26"
          },
        },
        {
          "scope": [
            "variable.other.enummember"
          ],
          "settings": {
            "foreground": "#83a598"
          },
        },
        {
          "scope": [
            "comment"
          ],
          "settings": {
            "fontStyle": ""
          },
        }
      ]
    },
    "[One Dark Pro]": {
      "textMateRules": [
        {
          "scope": [
            "support.function.go",
            "support.function.builtin.go"
          ],
          "settings": {
            "foreground": "#61afef"
          }
        },
        {
          "scope": [
            "keyword.operator.assignment.go",
            "variable.other.declaration.go",
            "variable.other.assignment.go"
          ],
          "settings": {
            "foreground": "#abb2bf"
          }
        },
        {
          "scope": [
            "storage.type"
          ],
          "settings": {
            "foreground": "#e5c07b"
          }
        }
      ]
    }
  },
  //
  "cSpell.userWords": [
    "Actix",
    "CMDB",
    "Deque",
    "Echarts",
    "Hongjie",
    "Mutex",
    "Spex",
    "Starpact",
    "Warnf",
    "Xlsx",
    "awsl",
    "azip",
    "bakana",
    "busybox",
    "cdylib",
    "cmath",
    "colormap",
    "consts",
    "curr",
    "erfc",
    "eval",
    "ffmpeg",
    "garena",
    "gomnd",
    "gonic",
    "ictx",
    "interp",
    "libc",
    "libm",
    "nalgebra",
    "ncols",
    "ndarray",
    "nolint",
    "nrows",
    "onclick",
    "oneshot",
    "postprocess",
    "powf",
    "preload",
